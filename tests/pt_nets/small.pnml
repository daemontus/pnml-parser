<?xml version="1.0"?>
<pnml xmlns="http://www.pnml.org/version-2009/grammar/pnml">
  <net id="small-net" type="http://www.pnml.org/version-2009/grammar/ptnet">
  	<name><text>P/T Net with one place</text></name>
    <page id="page0">
    	<place id="place">
    		<name>
    			<text>Some random place</text>
    		</name>
    		<initialMarking>
    			<text>100</text>
    		</initialMarking>
    	</place>
      <transition id="transition">
        <name>
          <text>Some random transition</text>
        </name>
      </transition>
      <arc source="transition" target="place" id="arc1">
        <inscription>
          <text>12</text>
        </inscription>
      </arc>
      <arc source="place" target="transition" id="arc2">
        <inscription>
          <text>3</text>
        </inscription>
      </arc>
    </page>
  </net>
</pnml>
